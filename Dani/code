def findHints(player1, move1, card1, player2, move2, card2):
    for i in strategyDatabase:
        if (i[4] ==player1 and i[7] == player2):
            if i[5] ==move1 and i[6]==card1 and i[8]==move2 and i[9]==card2:
                print("First person give person", i[0], " a hint about a", i[1],".", "Second person give person", i[2], "a hint about a", i[3], ".")
                return (i[0], i[1], i[2], i[3])
        elif (i[4] ==player2 and i[7] ==player1):
            if i[5] ==move2 and i[6]==card2 and i[8]==move1 and i[9]==card1:
                print("First person give person", i[0], " a hint about a", i[1],".", "Second person give person", i[2], "a hint about a", i[3], ".")
                return (i[0], i[1], i[2], i[3])
    print("No hint found. Sorry!")
    











 
